      url('~/fonts/helvetica/HelveticaNeueLTStd-It.eot?#iefix') format('embedded-opentype'),
      url('~/fonts/helvetica/HelveticaNeueLTStd-It.woff') format('woff'),
      url('~/fonts/helvetica/HelveticaNeueLTStd-It.ttf') format('truetype'),
      url('~/fonts/helvetica/HelveticaNeueLTStd-It.svg#HelveticaNeueLTStd-It') format('svg')
  font-weight normal
  font-style italic

